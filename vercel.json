{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/ai-zapier",
      "dest": "/ai-zapier"
    },
    {
      "src": "/api/workflows/(.*)",
      "dest": "/api/workflows/$1"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_BASE_URL": "https://chaincron-ai-zapier.vercel.app"
  },
  "functions": {
    "app/api/workflows/route.ts": {
      "maxDuration": 30
    },
    "app/api/workflows/execute/route.ts": {
      "maxDuration": 60
    },
    "app/api/workflows/share/route.ts": {
      "maxDuration": 30
    }
  }
}
